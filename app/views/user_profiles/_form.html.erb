<%= simple_form_for(@user_profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :name, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :starsign, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :hobbies, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :age, input_html: { class: 'form-control'} %>
  </div>

  <div class="form-group">
    <%= f.button :submit %>
  </div>
<% end %>
